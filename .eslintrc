{
  "extends": "@antfu",
  "ignorePatterns": [
    "./src/**/*.{ts,mjs,cjs,js}"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/space-before-blocks": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "react/prop-types": "off"
  }
}
