{
  "name": "@sadeghbarati/gatsby-plugin-smooth-scrollbarr",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "smooth-scrollbar plugin for gatsby",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-smooth-scrollbar"
  ],
  "homepage": "https://github.com/smooth-scrollbar/gatsby-plugin-smooth-scroll#readme",
  "bugs": {
    "url": "https://github.com/smooth-scrollbar/gatsby-plugin-smooth-scroll/issues"
  },
  "license": "MIT",
  "author": "@sadeghbarati",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smooth-scrollbar/gatsby-plugin-smooth-scroll.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    "./*": "./*",
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./plugins/overscroll": {
      "import": "./plugins/overscroll.mjs",
      "require": "./plugins/overscroll.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0",
    "smooth-scrollbar": "8.7.4"
  },
  "peerDependenciesMeta": {
    "gsap": {
      "optional": true
    }
  },
  "dependencies": {
    "ssr-window": "^4.0.2"
  },
  "optionalDependencies": {
    "gsap": "^3.10.4"
  },
  "devDependencies": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "es6-promise": "^4.2.8",
    "react": "17.0.2",
    "read-pkg": "^7.1.0",
    "smooth-scrollbar": "8.7.4",
    "unbuild": "0.7.4"
  }
}
